{
  "expo": {
    "name": "DriverUK",
    "slug": "MyApp2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "Messages"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rahmatullah95.MyApp2"
    },
    "android": {
      "permissions": [
        "INTERNET",
        "POST_NOTIFICATIONS",
        "NOTIFICATIONS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS",
        "com.android.vending.BILLING"
      ],
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rahmatullah95.MyApp2",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "driveruk",
              "host": "driveruk.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "scheme": "driveruk",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1ddbb52c-2865-49d0-af78-0efb8e2b98cd"
      },
      "firebaseApiKey": "@env:FIREBASE_API_KEY",
      "firebaseAuthDomain": "@env:FIREBASE_AUTH_DOMAIN",
      "firebaseProjectId": "@env:FIREBASE_PROJECT_ID",
      "firebaseStorageBucket": "@env:FIREBASE_STORAGE_BUCKET",
      "firebaseMessagingSenderId": "@env:FIREBASE_MESSAGING_SENDER_ID",
      "firebaseAppId": "@env:FIREBASE_APP_ID",
      "firebaseMeasurementId": "@env:FIREBASE_MEASUREMENT_ID",
      "googleServicesJsonBase64": "@env.GOOGLE_SERVICES_JSON_BASE64"
    },
    "plugins": ["react-native-iap", "expo-notifications"]
  }
}
